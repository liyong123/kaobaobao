/* pages/echarts/index.wxss */
.topBkg{
  display: flex;
  flex-direction: flex;
  height: 275rpx;
  background-color: #597FF3 ;
  justify-content: center;
}
.tabContent{
  height: 56rpx;
  margin-top:20rpx;
  background-color:transparent;
  width: 100%;
  padding: 0 40rpx 0 34rpx;
  /* width: 90%; */
}
.tabContent > view{
  display: inline-block;
  width:175rpx;
  height:52rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color:#fff;
  border:solid 2rpx #fff;
  border-radius: 28rpx;
  text-align: center;
  vertical-align: middle;
}
.tabContent > view:nth-child(3){
  margin:0 10rpx
}
.tabContent view.choosedTab {
  color:#2A57E8;
  background-color: #fff;
}
.tabContent .yearGrp{
  width: 110rpx;
  height: 50rpx;
  border: none;
  text-align: left;
  position: relative;
}
.tabContent .yearGrp text.showYearText{
  width: 14rpx;
  height: 18rpx;
  margin-right: 10rpx;
}
.tabContent .yearGrp text.showYearText2{
  width: 18rpx;
  height: 14rpx;
}
.yearGrpBoard{
  position: absolute;
  top: 62rpx;
  left:-6rpx;
  width: 356rpx;
  background: #fff;
  box-shadow: 0 0 8rpx 0 rgba(137,144,165,0.28);
  border-radius: 12rpx;
  z-index: 998
}
.yearGrpBoard{
  display: flex;
  flex-direction: column
}
.yearGrpBoard > view{
  display: flex;
}
.yearGrpBoard .yearGrpSelected{
   display: block;

}
.yearGrpSelected >view{
  display: inline-block;
  color: #AAAFDC;
  width: 118rpx;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-weight: bold;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
}
.yearGrpSelected > view.selectedYear{
  color: #54598B;
}
.yearGrpTitle{
  padding-left: 28rpx;
  display: flex;
  height: 54rpx;
  background-color: #E2E9FF;
  align-items: center;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx
}
.tabContent .yearGrpTitle >image{
  width: 24rpx;
  height: 26rpx;
}
.yearGrpTitle >text{
  font-size: 24rpx;
  color: #54598B;
}

.mainContent{
  position: relative;
}
.totalDataContent{
  z-index: 990;
  box-shadow:0rpx 10rpx 10rpx #dcdcdc;
  display: flex;
  flex-direction: column;
  width: 696rpx;
  height:360rpx;
  border-radius: 8rpx;
  position:absolute;
  top:-180rpx;
  left:50%;
  margin-left: -348rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,')
}
.totalArea{
  display: flex;
