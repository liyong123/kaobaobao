   margin-top:6rpx;
   font-size: 28rpx;
   color: #6E73A5;
   font-weight:bold
}
 .totalNum2 view:last-child view{
    width:4rpx;
    height:25rpx;
    background-color:#FFA000;
    margin-top:8rpx;
    margin-right:6rpx;
}
.stateGrp{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height:180rpx;
}
.stateGrp2{
