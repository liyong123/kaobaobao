}
 .totalNum2 image{
   width:146rpx;
   height:57rpx;
}
.totalNum text:last-child{
   margin-top:6rpx;
   font-size: 40rpx;
   color: #3B3FD9;
   font-weight:bold
}
 .totalNum2 text:first-child{
   width: 146rpx;
   height:57rpx;
   display: flex;
   font-size: 54rpx;
   color: #8D92BF;
   font-weight:bold
}
 .totalNum2 view:last-child{
   display: flex;
