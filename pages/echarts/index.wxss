  height: 165rpx;
  align-items: center;
  margin-top:50rpx;
}
.totalImg > image{
  width: 160rpx;
  height:160rpx;
}
 .totalNum{
  display: flex;
  flex-direction: column;
  margin-left:20rpx;
}
 .totalNum2{
  display: flex;
  flex-direction: column;
  margin-left:20rpx;
}
 .totalNum text:first-child{
   font-size: 28rpx;
   color: #7A848E;
